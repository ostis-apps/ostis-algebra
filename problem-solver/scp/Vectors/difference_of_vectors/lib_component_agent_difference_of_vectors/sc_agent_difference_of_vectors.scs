sc_agent_difference_of_vectors
=> nrel_main_idtf:
	[sc-агент нахождения разности векторов] (* <- lang_ru;; *);
	[sc-agent of finding difference of vectors] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result:
	(..sc_agent_difference_of_vectors_condition => ..sc_agent_difference_of_vectors_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_difference_of_vectors
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_difference_of_vectors
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_difference_of_vectors
		};;
	-> sc_agent_difference_of_vectors_scp    
		(* <- active_sc_agent;; *);; 
	*);;

..sc_agent_difference_of_vectors
= [*
	question_difference_of_vectors _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_difference_of_vectors_result
= [*
	question_difference_of_vectors _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
