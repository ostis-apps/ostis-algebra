sc_agent_exponentiation
=> nrel_main_idtf:
	[sc-агент нахождение произведения 2-х комплексных чисел] (* <- lang_ru;; *);
	[sc-agent finding multiplication complex number] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_multiplication_complex_number_condition => ..sc_agent_multiplication_complex_number_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_multiplication_complex_number
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_multiplication_complex_number
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_multiplication_complex_number
		};;
	-> sc_agent_multiplication_complex_number_scp    
		(* <- active_sc_agent;; *);; 
	*);;

..sc_agent_multiplication_complex_number
= [*
	question_multiplication_complex_number _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_multiplication_complex_number_result
= [*
	question_multiplication_complex_number _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
