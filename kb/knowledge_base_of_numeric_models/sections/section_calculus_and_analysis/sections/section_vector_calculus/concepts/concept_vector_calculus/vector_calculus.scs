concept_vector_calculus <- rrel_key_sc_element:
	...
	(*
	=> nrel_main_idtf: [Опр. (векторное исчисление)] (*<-lang_ru;;*);;
	<-sc_definition;;
	<= nrel_sc_text_translation:
		...
		(*
		-> rrel_definition:
			[Векторное исчисление — раздел математического анализа, в котором изучаются свойства операций над векторами.](*<-lang_ru;;*);;
		*);;
	*);;
	

//используемые константы
concept_vector_calculus <= nrel_using_constants:
	{
	concept_mathematical_analysis;
	concept_vector
	};;
	
//утверждения
concept_vector_calculus <- rrel_key_sc_element:
	... 
	(*
	=> nrel_main_idtf: [Утв.(векторное исчисление; тензорное исчисление)] (* <- lang_ru;; *);;
	<- sc_statement;;
	<= nrel_sc_text_translation: 
		...
		(*
		-> rrel_example: 
			[Расширением векторного исчисления является тензорное исчисление.](* <- lang_ru;; *);;
		*);;
	*);
	... 
	(*
	=> nrel_main_idtf: [Утв.(векторное исчисление; линейное интегральное уравнение; дифференциальное уравнение; алгебраическая геометрия)] (* <- lang_ru;; *);;
	<- sc_statement;;
	<= nrel_sc_text_translation: 
		...
		(*
		-> rrel_example: 
			[Методы, основанные на векторном исчислении, нашли широкое применение в теории линейных интегральных уравнений, в теории обработки сигналов, в теории обыкновенных дифференциальных уравнений, алгебраической геометрии и т.д.](* <- lang_ru;; *);;
		*);;
	*);;
