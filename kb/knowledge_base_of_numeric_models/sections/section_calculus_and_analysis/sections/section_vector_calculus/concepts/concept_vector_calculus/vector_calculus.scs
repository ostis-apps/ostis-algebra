concept_vector_calculus <= nrel_subdividing:
	...
	(* 
	-> concept_linear_algebra;;
	-> concept_vector_analysis;;
	-> concept_functional_analysis;;
	*);;

//надмножество
concept_mathematical_analysis => nrel_inclusion: concept_vector_calculus;;

