number_googol => nrel_main_idtf:
[гугол](*<-lang_ru;;*);
[googol](*<-lang_en;;*);;

//синонимы
number_googol => nrel_idtf:
[10 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000 000](*<-lang_ru;;*);
[самое большое число](*<-lang_ru;;*);;

number_googol<-concept_number;;

//определение
number_googol <- rrel_key_sc_element:
	...
	(*
	=> nrel_main_idtf: [Опр. (гугол)] (*<-lang_ru;;*);;
	<-sc_definition;;
	<= nrel_sc_text_translation:
		...
		(*
		-> rrel_definition:
			[Гугол - число, в десятичной системе счисления изображаемое единицей со 100 нулями.](*<-lang_ru;;*);;
		*);;
	*);;

//утверждения
number_googol <- rrel_key_sc_element:
	... 
	(*
	=> nrel_main_idtf: [Утв.(гугол; число; бесконечность; математика)] (* <- lang_ru;; *);;
	<- sc_statement;;
	<= nrel_sc_text_translation: 
		...
		(*
		-> rrel_example: 
			[Термин «гугол» не имеет серьёзного теоретического и практического значения. Казнер предложил его для того, чтобы проиллюстрировать разницу между невообразимо большим числом и бесконечностью, и с этой целью термин иногда используется при обучении математике.](* <- lang_ru;; *);;
		*);;
	*);
	... 
	(*
	=> nrel_main_idtf: [Утв.(гугол)] (* <- lang_ru;; *);;
	<- sc_statement;;
	<= nrel_sc_text_translation: 
		...
		(*
		-> rrel_example: 
			[Гугол больше, чем количество атомов в известной нам части Вселенной, что также ограничивает его применение.](* <- lang_ru;; *);;
		*);;
	*);;
